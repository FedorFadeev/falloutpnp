        <td>
          <input
            type="checkbox"
            name="system.skills.smallGuns.isTagged"
            {{checked system.skills.smallGuns.isTagged}}
            {{disabled (ifThen system.skills.smallGuns.isTagged false system.isTaggingDisabled)}}
          />
        </td>
        <td>
          <div class="skill-icon">
            <img src="systems/falloutpnp/static/icons/icon-skill-smallGuns.png" alt="Small Guns" />
          </div>
        </td>
        <td>
          <p>Small Guns</p>
        </td>
        <td title="5+AGx4">
          <div class="styled-div_input-like">
            {{system.skills.smallGuns.base}}
          </div>
        </td>
        <td class="skills-cell skills-cell_small">
          <input
            type="number"
            name="system.skills.smallGuns.skillPoints"
            value="{{system.skills.smallGuns.skillPoints}}"
