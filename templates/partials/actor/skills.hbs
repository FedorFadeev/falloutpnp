<h2>Skills</h2>
{{#if system.general.remainingSkillPoints}}
  <p class="skills-unassigned">Unassigned: {{system.general.remainingSkillPoints}}</p>
{{/if}}
<div class="skills-list">
  <table>
    <tr>
      <th><p>Tag</p></th>
      <th><p>&nbsp;</p></th>
      <th><p>Skill</p></th>
      <th><p>Base</p></th>
      <th><p>Skill Points</p></th>
      <th><p>+</p></th>
      <th><p>-</p></th>
      <th><p>Total</p></th>
    </tr>
    <tr>
      <td>
        <input
          type="checkbox"
          name="system.skills.smallGuns.isTagged"
          {{checked system.skills.smallGuns.isTagged}}
          {{disabled (ifThen system.skills.smallGuns.isTagged false system.isTaggingDisabled)}}
        />
      </td>
      <td>
        <div class="vault-boy">
          <img src="systems/falloutpnp/static/icons/icon-skill-smallGuns.png" alt="Small Guns" />
        </div>
      </td>
      <td>
        <p class="skill-name">Small Guns</p>
      </td>
      <td title="5+AGx4">
        <div class="data-field">
          {{system.skills.smallGuns.base}}
        </div>
      </td>
      <td>
        <div class="number-input-field">
          <input
            type="number"
            name="system.skills.smallGuns.skillPoints"
            value="{{system.skills.smallGuns.skillPoints}}"
          />
        </div>
      </td>
      <td>
        <div class="number-input-field">
          <input
            type="number"
            name="system.skills.smallGuns.diffPositive"
            value="{{system.skills.smallGuns.diffPositive}}"
